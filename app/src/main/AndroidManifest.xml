<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="droidsetter.randomquiz">
<uses-permission android:name="android.permission.INTERNET"/>
    <application
        android:allowBackup="true"
        android:icon="@drawable/randomquiz"
        android:label="@string/app_name"
        android:roundIcon="@drawable/randomquiz"
        android:supportsRtl="true"
        android:theme="@style/Theme.RandomQuiz">
        <activity
            android:name="com.droidsetter.randomquiz.GameRuleActivity"
            android:exported="true" />
        <activity
            android:name="com.droidsetter.randomquiz.PrivacyActivity"
            android:exported="true" />
        <activity
            android:name="com.droidsetter.randomquiz.MenuActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.droidsetter.randomquiz.MainActivity"
            android:exported="true">
        </activity>

        <meta-data
            android:name="preloaded_fonts"
            android:resource="@array/preloaded_fonts" />
    </application>

</manifest>